<mvc:View controllerName="sap.ui.Shop.controller.SupplierInfo"
	xmlns:m="sap.m"
	xmlns:f="sap.f"
	xmlns:mvc="sap.ui.core.mvc"
	height="100%">
	<f:FlexibleColumnLayout
		layout="TwoColumnsMidExpanded">
		<!-- Products -->
		<f:beginColumnPages>
			<!-- Products -->
			<mvc:XMLView
				id="SupplierProducts"
				viewName="sap.ui.Shop.view.SupplierInfoProducts"/>
		</f:beginColumnPages>
		<!-- Product Info -->
		<f:midColumnPages>
			<mvc:XMLView
				id="SupplierProductInfo"
				viewName="sap.ui.Shop.view.SupplierInfoProducts"/>
		</f:midColumnPages>

	</f:FlexibleColumnLayout>
</mvc:View>