<mvc:View controllerName="sap.ui.Shop.controller.SupplierInfo"
	displayBlock="true"
	height="100%"
	xmlns="sap.f"
	xmlns:mvc="sap.ui.core.mvc">
	<FlexibleColumnLayout
		layout="TwoColumnsMidExpanded"
		id="SipplierInfoLayout"
		backgroundDesign="Solid">
		<!-- Products -->
		<beginColumnPages>
			<mvc:XMLView
				id="SupplierProducts"
				viewName="sap.ui.Shop.view.SupplierProductList"/>
		</beginColumnPages>
		<!-- Product Info -->
		<midColumnPages>
			<!-- <mvc:XMLView
				viewName="sap.ui.Shop.view.SupplierProductList"/> -->
			<mvc:XMLView
				id="SupplierProductInfo"
				viewName="sap.ui.Shop.view.SupplierProductInfo"/>
		</midColumnPages>

	</FlexibleColumnLayout>
</mvc:View>