<mvc:View 
    xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.uxap"
	xmlns:f="sap.f"
	xmlns:m="sap.m">
	<ObjectPageLayout
		showTitleInHeaderContent="true"
		showEditHeaderButton="true"
		upperCaseAnchorBar="false"
		editHeaderButtonPress="handleEditBtnPress">
		<headerTitle>
			<ObjectPageDynamicHeaderTitle>
				<!-- Title -->
				<heading>
					<m:FlexBox
						fitContainer="true"
						alignItems="Center">
						<f:Avatar
							src=""
							class="sapUiTinyMarginEnd"/>
							<!-- Name -->
						<m:Title
							text=""
							wrapping="true" />
					</m:FlexBox>
				</heading>
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>
		<headerContent>
		</headerContent>

		<!-- Content -->
		<sections>
			<ObjectPageSection
				titleUppercase="false"
				title="{i18n>productTitleAboutProduct}">
				<subSections>
					<ObjectPageSubSection title="About Product">
						<m:Text text="asdsadas" />
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection
				titleUppercase="false"
				showTitle="false"
				title="{i18n>productTitleSuppliers}">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<m:Text text="asdsad" />
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>

		<footer>
		</footer>
	</ObjectPageLayout>
</mvc:View>